{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@json/*": [
        "src/json/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ]
    },
    "jsx": "preserve",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
  }
}